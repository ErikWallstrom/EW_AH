build: 
	gcc ../../src/Main.c ../../src/Array.c ../../src/Script.c ../../src/Libraries.c ../../src/Entity.c ../../src/Error.c ../../src/Graphics_Component.c ../../src/Event_Component.c -Wall -Wextra -Wshadow -Wfatal-errors -g -o Program -std=c99 -lSDL2 -lSDL2_net -lSDL2_ttf -lSDL2_mixer -lSDL2_image -lm -llua5.3

debug:
	gdb ./Program

run:
	./Program
